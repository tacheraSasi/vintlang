// Simple test to check if debounce function is available
println("Testing if debounce function exists...")

// Test basic function call
let testFunc = func() {
    println("Test function executed!")
}

// Try to create a debounced version
let debouncedTest = debounce(100, testFunc)
println("Debounce function created successfully!")
println("Type of debounced function:", type(debouncedTest))