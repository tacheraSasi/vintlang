// Test string builtin functions

// Test format function
let formatted = format("Hello %s, you are %d years old", "Alice", 25);
println("Format test:", formatted);

// Test startsWith function
let text = "Hello World";
let starts = startsWith(text, "Hello");
println("StartsWith test:", starts);

let notStarts = startsWith(text, "World");
println("StartsWith false test:", notStarts);

// Test endsWith function
let ends = endsWith(text, "World");
println("EndsWith test:", ends);

let notEnds = endsWith(text, "Hello");
println("EndsWith false test:", notEnds);

// Test chr function
let character = chr(65); // ASCII 'A'
println("Chr test:", character);

// Test ord function
let asciiValue = ord("A");
println("Ord test:", asciiValue);

// Test with existing len function for comparison
let length = len("Hello World");
println("Len test:", length);